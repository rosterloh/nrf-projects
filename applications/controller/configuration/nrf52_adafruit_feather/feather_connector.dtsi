/ {
	feather_header: connector {
		compatible = "adafruit-feather";
		#gpio-cells = <2>;
		gpio-map-mask = <0xffffffff 0xffffffc0>;
		gpio-map-pass-thru = <0 0x3f>;
		gpio-map = <2  0 &gpio0 2  0>,	/* A0 */
			   <3  0 &gpio0 3  0>,	/* A1 */
			   <4  0 &gpio0 4  0>,	/* A2 */
			   <5  0 &gpio0 5  0>,	/* A3 */
			   <28 0 &gpio0 28 0>,	/* A4 */
			   <29 0 &gpio0 29 0>,	/* A5 */
			   <12 0 &gpio0 12 0>,	/* D0 */
			   <13 0 &gpio0 13 0>,	/* D1 */
			   <14 0 &gpio0 14 0>,	/* D2 */
			   <8  0 &gpio0 8  0>,	/* D3 */
			   <6  0 &gpio0 6  0>,	/* D4 */
			   <20 0 &gpio0 20 0>,	/* D5 */
			   <25 0 &gpio0 15 0>,	/* D6 */
			   <26 0 &gpio0 26 0>,	/* D7 */
			   <27 0 &gpio0 27 0>,	/* D8 */
			   <30 0 &gpio0 30 0>,	/* D9 */
			   <31 0 &gpio0 31 0>,	/* D10 */
			   <11 0 &gpio0 11 0>,	/* D11 */
			   <7  0 &gpio0 7  0>,	/* D12 */
			   <15 0 &gpio0 15 0>,	/* D13 */
			   <16 0 &gpio0 16 0>,	/* D14 */
	};
};